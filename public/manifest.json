{
  "manifest_version": 3,
  "name": "Awesome Bookmark",
  "description": "A browser bookmark manager with React and Shadcn UI.",
  "version": "1.0",
  "chrome_url_overrides" : {
    "bookmarks": "index.html"
  },
  "action": {
    "default_title": "This extension has taken over your bookmark manager",
    "default_icon": {
      "16": "extension_icon@16px.png",
      "32": "extension_icon@32px.png",
      "48": "extension_icon@48px.png",
      "128": "extension_icon@128px.png"
    }
  },
  "icons": {
    "16": "extension_icon@16px.png",
    "32": "extension_icon@32px.png",
    "48": "extension_icon@48px.png",
    "128": "extension_icon@128px.png"
  },
  "permissions": ["bookmarks", "favicon"],
  "host_permissions": ["<all_urls>"]
}
